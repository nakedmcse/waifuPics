{
  "name": "waifupics",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "tsc": "tsc --project tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "build": "node ./buildScripts/cleanDist.mjs && swc src -d dist --config-file .swcrc --copy-files && node ./buildScripts/moveFiles.mjs",
    "update_deps": "npx npm-check-updates",
    "start:dev": "node --loader ts-node/esm src/app.ts",
    "start:prod": "node dist/app.js",
    "prepare": "husky"
  },
  "dependencies": {
    "cookie-parser": "~1.4.6",
    "debug": "~4.3.7",
    "dotenv": "^16.4.5",
    "ejs": "~3.1.10",
    "express": "~4.21.0",
    "file-type": "^19.5.0",
    "http-errors": "~2.0.0",
    "mime": "^4.0.4",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.26",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "eslint": "^9.11.0",
    "prettier": "3.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.6.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "tsc-files": "^1.1.4"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tsc-files --noEmit --pretty",
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
